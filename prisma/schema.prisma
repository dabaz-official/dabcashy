generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  email String? @unique
  password String?
  first_name String?
  last_name String?
}

enum Role {
  USER
  ADMIN
}

model Expense {
  id String @id
  ammount Decimal
  category String?
  createdAt DateTime @default(now())
}